

<TabbedPanelItem>
    markup: True
    background_normal: ''
    background_color: (128/255, 64/255, 0, 1)
    bold: True

<Button>
    markup: True
    bold: True
    font_size: 16
    background_normal: ''
    background_color: (128/255, 77/255, 0, 1)

<StandardButton@Button>
    background_color: (0, 0, 0, 0)
    col: (128/255, 77/255, 0, 1)
    on_press: self.col = (30/255, 38/255, 0, 1)
    on_release: self.col = (128/255, 77/255, 0, 1)
    canvas.before:
        Color:
            rgba: self.col
        RoundedRectangle:
            size: self.size
            pos: self.pos
            radius: [8]

<ResetButton@Button>
    background_color: (0, 0, 0, 0)
    col: (0/255, 77/255, 0, 1)
    on_press: self.col = (30/255, 38/255, 0, 1)
    on_release: self.col = (0/255, 77/255, 0, 1)
    canvas.before:
        Color:
            rgba: self.col
        RoundedRectangle:
            size: self.size
            pos: self.pos
            radius: [8]

