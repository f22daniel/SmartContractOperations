<GanachePopup>
    auto_dismiss: True
    markup: True
    size_hint: 0.6, 0.6
    pos_hint: {'center': 0, 'top': .8}
    title: 'Ganache'
    valign: 'middle'
    canvas.before:
        Color:
            rgba: (1, 166 / 255, 77 / 255, 1)
        Rectangle:
            size: self.size
            pos: self.pos
    BoxLayout:
        size: root.width, root.height
        orientation: 'vertical'
        spacing: 2
        ScrollView:
            do_scroll_x: False
            do_scroll_y: True
            GridLayout:
                orientation: 'lr-tb'
                id: ganache_address_list
                size_hint_y: None
                height: self.minimum_height
                cols: 3
                spacing: 2.5
        LabelDescription:
            id: ganache_status
            size_hint: 1, 0.2
        Button:
            size_hint: 1, .2
            text: 'Close'
            on_release: root.dismiss()
            background_color: (0, 0, 0, 0)
            col: (204/255, 0, 0, 1)
            on_press: self.col = (30/255, 38/255, 0, 1)
            on_release: self.col = (204/255, 0, 0, 1)
            canvas.before:
                Color:
                    rgba: self.col
                RoundedRectangle:
                    size: self.size
                    pos: self.pos
                    radius: [8]



<SettingsPopup>
    auto_dismiss: True
    markup: True
    size_hint: 0.5, 0.5
    pos_hint: {'center': 0, 'top': .8}
    title: 'Settings'
    valign: 'middle'
    canvas.before:
        Color:
            rgba: (1, 166 / 255, 77 / 255, 1)
        Rectangle:
            size: self.size
            pos: self.pos
    BoxLayout:
        size: root.width, root.height
        orientation: 'vertical'
        GridLayout:
            spacing: 2
            padding: 2
            rows: 5
            cols: 2
            orientation: 'lr-tb'
            LabelDescription:
                text: 'Etherscan API'
                size_hint: 0.35, 1
            PopupInput:
                text: ''
                id: APIEnter
                on_focus: root.api_enter_popup(self.focus, self.text)
            LabelDescription:
                text: 'BSC API'
                size_hint: 0.35, 1
            PopupInput:
                text: ''
                id: BSCAPIEnter
                on_focus: root.bsc_api_enter_popup(self.focus, self.text)
            LabelDescription:
                text: 'Infura API'
                size_hint: 0.35, 1
            PopupInput:
                text: ''
                id: InfuraEnter
                on_focus: root.infura_enter_popup(self.focus, self.text)
            LabelDescription:
                text: 'Ganache RPC'
                size_hint: 0.35, 1
            PopupInput:
                text: ''
                id: GanacheRPCEnter
                on_focus: root.ganache_rpc_enter_popup(self.focus, self.text)
            LabelDescription:
                text: 'Ganache ID'
                size_hint: 0.35, 1
            PopupInput:
                text: ''
                id: GanacheIDEnter
                on_focus: root.ganache_id_enter_popup(self.focus, self.text)
        StandardButton:
            size_hint: 1, .2
            text: 'Close'
            on_release: root.dismiss()

<AddressPopup>
    auto_dismiss: True
    markup: True
    size_hint: 0.7, 0.42
    pos_hint: {'center': 0, 'top': .8}
    title: 'Set Address'
    valign: 'middle'
    canvas.before:
        Color:
            rgba: (1, 166 / 255, 77 / 255, 1)
        Rectangle:
            size: self.size
            pos: self.pos
    BoxLayout:
        size: root.width, root.height
        orientation: 'vertical'
        GridLayout:
            spacing: 2
            padding: 2
            rows: 2
            cols: 2
            orientation: 'lr-tb'
            PopupLabel:
                text: 'Address:'
                size_hint: 0.15, .7
            PopupInput:
                id: address_popup
                text: ''
                size_hint: 1, .7
                on_focus:
                    root.address_enter_popup(self.focus, self.text)
                    app.mylayout.address_enter(self.focus)
            PopupLabel:
                text: 'Private Key:'
                size_hint: 0.15, .7
            PopupInput:
                id: private_key_popup
                text: ''
                size_hint: 0.15, .7
                on_focus: root.private_key_enter_popup(self.focus, self.text)

        GridLayout:
            spacing: 3
            padding: 3
            rows: 1
            cols: 6
            orientation: 'lr-tb'
            size_hint: 1, .5
            PopupToggle:
                id: Address1
                text: 'Address 1'
                on_press: root.select_address(self.text)
                on_release: app.mylayout.address_selection()
            PopupToggle:
                id: Address2
                text: 'Address 2'
                on_press: root.select_address(self.text)
                on_release: app.mylayout.address_selection()
            PopupToggle:
                id: Address3
                text: 'Address 3'
                on_press: root.select_address(self.text)
                on_release: app.mylayout.address_selection()
            PopupToggle:
                id: Address4
                text: 'Address 4'
                on_press: root.select_address(self.text)
                on_release: app.mylayout.address_selection()
            PopupToggle:
                id: Address5
                text: 'Address 5'
                on_press: root.select_address(self.text)
                on_release: app.mylayout.address_selection()
            PopupToggle:
                id: Address6
                text: 'Address 6'
                on_press: root.select_address(self.text)
                on_release: app.mylayout.address_selection()

        StandardButton:
            text: 'Close'
            on_release: root.dismiss()
            size_hint: 1, 0.5


<NetworkPopup>
    auto_dismiss: True
    markup: True
    size_hint: 0.3, 0.4
    pos_hint: {'center': 0, 'top': .8}
    title: 'Select Network'
    valign: 'middle'
    BoxLayout:
        size: root.width, root.height
        orientation: 'vertical'
        spacing: 3
        padding: 3
        NetworkButton:
            id: Mainnet
            text: 'Mainnet'
            on_press: app.mylayout.network_selection(Mainnet.text, 1)
            on_release: root.dismiss()
        NetworkButton:
            id: Goerli
            text: 'Goerli'
            on_press: app.mylayout.network_selection(Goerli.text, 5)
            on_release: root.dismiss()
        NetworkButton:
            id: Binance
            text: 'Binance Smart Chain'
            on_press: app.mylayout.network_selection(Binance.text, 56)
            on_release: root.dismiss()
        NetworkButton:
            id: Ganache
            text: 'Ganache'
            on_press: app.mylayout.network_selection(Ganache.text, '')
            on_release: root.dismiss()

