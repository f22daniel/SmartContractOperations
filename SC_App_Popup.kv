#:import Factory kivy.factory.Factory

<CompilerButton@ToggleButton>
    markup: True
    bold: True
    font_size: 18
    size_hint: 1, None
    height: 30
    group: 'compiler_version'
    background_normal: ''
    background_color: (255/255, 204/255, 128/255 , 1)

<GanacheAddressIndex@LabelDescription>
    font_size: 16
    size_hint: 0.25, 1
    bold: True
    col: (179/255, 89/255, 0 , 1)
    canvas.before:
        Color:
            rgba: self.col
        RoundedRectangle:
            size: self.size
            pos: self.pos
            radius: [8]

<GanacheAddress@CompilerButton>

<NetworkButton@CompilerButton>
    group: 'network_active'
    size_hint: 1, 1

<PopupInput@TextInput>
    font_size: 16
    size_hint: 1, 1

<PopupLabel@Label>
    markup: True
    bold: True
    font_size: 14
    col: (179/255, 89/255, 0 , 1)
    canvas.before:
        Color:
            rgba: self.col
        Rectangle:
            size: self.size
            pos: self.pos

<PopupToggle@ToggleButton>
    markup: True
    bold: True
    font_size: 14
    background_normal: ''
    background_color: (128/255, 77/255, 0, 1)
    group: 'AddressActive'

<CancelButton@PayableButton>

<ProceedButton@ViewButton>

<PrivateKeyPopup2>
    auto_dismiss: True
    markup: True
    size_hint: 0.7, 0.325
    pos_hint: {'center': 0, 'top': .6}
    title: 'Ganache Private Key'
    valign: 'middle'
    canvas.before:
        Color:
            rgba: (1, 166 / 255, 77 / 255, 1)
        Rectangle:
            size: self.size
            pos: self.pos
    BoxLayout:
        size: root.width, root.height
        orientation: 'vertical'
        spacing: 2
        BoxLayout:
            size: root.width, root.height
            spacing: 2
            padding: 2
            orientation: 'horizontal'
            size_hint: 1, 1
            LabelDescription:
                size_hint: 0.25, 1
                text: 'Enter Private Key: '
                font_size: 16
                bold: True
            TextInput:
                size_hint: 1, 1
                id: ganache_private_key_2
                font_size: 16
        BoxLayout:
            size: root.width, root.height
            size_hint:
            spacing: 2
            padding: 2
            orientation: 'horizontal'
            CancelButton:
                size_hint: 1, 1
                text: 'Cancel'
                on_release: root.dismiss()

            ProceedButton:
                size_hint: 1, 1
                text: 'Proceed'
                on_press: app.mylayout.ganache_private_key_enter(ganache_private_key_2.text)
                on_release:
                    root.dismiss()
                    app.mylayout.transact()

