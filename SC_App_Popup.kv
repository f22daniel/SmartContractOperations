#:import Factory kivy.factory.Factory

<CompilerButton@ToggleButton>
    markup: True
    bold: True
    font_size: 18
    size_hint: 1, None
    height: 30
    group: 'compiler_version'
    background_normal: ''
    background_color: (255/255, 204/255, 128/255 , 1)

<NetworkButton@CompilerButton>
    group: 'network_active'
    size_hint: 1, 1

<NetworkPopup@Popup>
    auto_dismiss: True
    markup: True
    size_hint: 0.3, 0.7
    pos_hint: {'center': 0, 'top': .8}
    title: 'Select Network'
    valign: 'middle'
    BoxLayout:
        size: root.width, root.height
        orientation: 'vertical'
        spacing: 3
        padding: 3
        NetworkButton:
            id: Mainnet
            text: 'Mainnet'
            on_press:
                app.root.ids.active_network.text = self.text
                app.popup_window.network_selection(Mainnet.text, 1)
        NetworkButton:
            id: Goerli
            text: 'Goerli'
            on_press:
                app.root.ids.active_network.text = self.text
                app.popup_window.network_selection(Goerli.text, 5)
        NetworkButton:
            id: Kovan
            text: 'Kovan'
            on_press:
                app.root.ids.active_network.text = self.text
                app.popup_window.network_selection(Kovan.text, 42)
        NetworkButton:
            id: Rinkeby
            text: 'Rinkeby'
            on_press:
                app.root.ids.active_network.text = self.text
                app.popup_window.network_selection(Rinkeby.text, 4)
        NetworkButton:
            id: Ropsten
            text: 'Ropsten'
            on_press:
                app.root.ids.active_network.text = self.text
                app.popup_window.network_selection(Ropsten.text, 3)
        NetworkButton:
            id: Ganache
            text: 'Ganache'
            on_press:
                app.root.ids.active_network.text = self.text
                app.popup_window.network_selection(Ganache.text, '')


<MyPopup@Popup>
    auto_dismiss: True
    markup: True
    size_hint: 0.3, 0.7
    pos_hint: {'center': 0, 'top': .8}
    title: 'Set Compiler'
    valign: 'middle'
    canvas.before:
        Color:
            rgba: (1, 166 / 255, 77 / 255, 1)
        Rectangle:
            size: self.size
            pos: self.pos
    BoxLayout:
        size: root.width, root.height
        orientation: 'vertical'
        Label:
            id: popup_label
            size_hint: 1, .1
            text: ''
            font_size: 20
            bold: True
        ScrollView:
            do_scroll_x: False
            do_scroll_y: True
            GridLayout:
                size_hint_y: None
                height: self.minimum_height
                spacing: 2.5
                padding: 2.5
